{
  "name": "utest",
  "version": "1.0.0",
  "description": "Command-line tool for finding and reporting dead links in a file",
  "bin": {
    "utest": "./Utest.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/egrinberg/Utest.git"
  },
  "scripts": {
    "eslint": "eslint --ignore-path .gitignore .",
    "eslint-fix": "eslint --fix --ignore-path .gitignore .",
    "lint": "npm run eslint",
    "prettier": "prettier --write \"./**/*.{md,jsx,json,html,css,js,yml}\"",
    "prettier-check": "prettier --check \"./**/*.{md,jsx,json,html,css,js,yml}\"",
    "test": "jest"
  },
  "keywords": [
    "cli"
  ],
  "preferGlobal": true,
  "author": "Ekaterina Grinberg",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/egrinberg/Utest/issues"
  },
  "homepage": "https://github.com/egrinberg/Utest#readme",
  "dependencies": {
    "boxen": "^4.0.0",
    "chalk": "^2.4.2",
    "node-fetch": "^2.6.1",
    "yargs": "^16.0.3"
  },
  "devDependencies": {
    "eslint": "7.13.0",
    "eslint-config-prettier": "6.15.0",
    "eslint-plugin-prettier": "3.1.4",
    "jest": "26.6.3",
    "prettier": "2.1.2"
  }
}
